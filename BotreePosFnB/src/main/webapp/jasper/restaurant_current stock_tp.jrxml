	<queryString>
		<![CDATA[select a.created_date,a.name,a.unit, t2.item_id, t2.quantity from im_m_inv_type_items_c a,

(select t.item_id, sum(t.item_quantity) as quantity from (

(select item_id, sum(item_quantity)  as item_quantity
from im_t_raw_stock_in_items_c where store_id=$P{W_StoreId} group by item_id )
union (select item_id, sum(ifnull(item_quantity*-1, 0)) from im_t_raw_stock_out_items_c
where store_id=$P{W_StoreId}  group by item_id )


)as t group by item_id) t2 where a.id=t2.item_id
;]]>
	</queryString>
