	<queryString>
		<![CDATA[select     s.store_name
          ,s.address
          ,s.phone_no phone
          ,s.email_id email
          ,s.currency currency_code

          ,x.inv_no
          ,x.inv_date
          ,x.inv_type
          ,x.bank_amount
          ,x.cash_amount
          ,x.balance_amount
          ,x.inv_status


from
(
    select  j.inv_no
           ,j.inv_date
           ,j.narration inv_type
           ,0 bank_amount
           ,jp.dr_amount cash_amount
           ,0 balance_amount
           ,'' inv_status


    from acc_t_journal j
    inner join acc_t_journal_post jp on j.id=jp.journal_id
    inner join acc_m_group g on jp.account_group_id=g.id
    where  j.inv_date>=$P{W_startDate} and j.inv_date <= $P{W_endDate}
    and j.store_id=$P{W_storeID}
    and jp.dr_amount > 0
    and g.code in ('CIH')

    union all

    select  j.inv_no
           ,j.inv_date
           ,j.narration inv_type
           ,jp.dr_amount bank_amount
           ,0 cash_amount
           ,0 balance_amount
           ,'' inv_status


    from acc_t_journal j
    inner join acc_t_journal_post jp on j.id=jp.journal_id
    inner join acc_m_group g on jp.account_group_id=g.id
    where  j.inv_date>=$P{W_startDate} and j.inv_date <= $P{W_endDate}
    and j.store_id=$P{W_storeID}
    and jp.dr_amount > 0
    and g.code in ('CAB')


)x
inner join sl_m_restaurant_store_c s on s.id=$P{W_storeID}
order by x.inv_date
;]]>
	</queryString>
