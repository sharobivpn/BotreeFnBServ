	<queryString>
		<![CDATA[select     x.ledger_id
          ,x.ledger_name
          ,x.store_name
          ,x.address
          ,x.phone
          ,x.email
          ,x.currency_code
          ,case
             when currentBalance >=0 then currentBalance
             else 0 end dr_amount
          ,case
             when currentBalance < 0 then abs(currentBalance)
             else 0 end cr_amount


from
(
    select a.id ledger_id
          ,concat(a.name,' - ',g.name ) ledger_name
          ,( select get_ledgerBalance_asOn( a.id,a.group_id,2,$P{W_storeID},$P{W_asOnDate} ) ) currentBalance
          ,g.name group_name
          ,s.store_name
          ,s.address
          ,s.phone_no phone
          ,s.email_id email
          ,s.currency currency_code

    from acc_m_account a
    inner join acc_m_group g on a.group_id=g.id
    inner join sl_m_restaurant_store_c s on a.store_id=s.id
    where  a.store_id = $P{W_storeID}
    and s.id=$P{W_storeID}

)x
 where (
           ( abs(x.currentBalance)>0 and ifnull($P{W_IsExcludingZero},0) =1 )
           or
           (ifnull($P{W_IsExcludingZero},0) =0)
       )
order by x.group_name,x.ledger_name
;]]>
	</queryString>
