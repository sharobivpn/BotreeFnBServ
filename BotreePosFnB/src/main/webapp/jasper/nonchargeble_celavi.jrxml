	</subDataset>
	<parameter name="W_StartDate" class="java.lang.String"/>
	<parameter name="W_StoreId" class="java.lang.String"/>
	<parameter name="W_EndDate" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT f.id
      ,case
        when sr.order_date is null then f.order_date
        else cast(sr.open_time as date) end order_date
      ,substring(f.order_time,-8) order_time
      ,b.customer_discount
      ,b.discount_percentage
      ,c.name

      ,b.gross_amt
      ,ifnull(b.discount_reason,'') discount_reason
FROM fo_t_orders f
inner join bp_t_bill b on f.id=b.order_id
inner join ci_t_customer c on f.person_id=c.id
left join st_t_day_book_reg sr on f.store_id=sr.store_id
where
b.discount_percentage=100
and f.store_id=$P{W_StoreId}
and (
      (
          (f.order_date  between  $P{W_StartDate} and $P{W_EndDate}  )
          and
          (sr.order_date is null)
      )
      or
     (
         ( STR_TO_DATE(f.order_time, '%d/%m/%Y %T') between sr.open_time and ifnull(sr.close_time,now()) )
         and
         ( cast(sr.open_time as date) between  $P{W_StartDate} and $P{W_EndDate} )
         and
         (sr.order_date is not null)
     )
   )


;]]>
	</queryString>
