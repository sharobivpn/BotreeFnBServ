	<queryString>
		<![CDATA[SELECT si.date,iti.name,sc.item_quantity,iti.unit,sc.item_total_price FROM restaurant.im_t_inv_stock_in si
inner join im_t_inv_stock_in_items_c sc on si.id=sc.stock_in_id
left join im_m_inv_type_items_c iti on sc.item_id=iti.id
where si.date  between $P{W_StartDate} and $P{W_EndDate} and si.store_id=$P{W_StoreId}
group by WEEK(si.date)
order by WEEK(si.date) asc;]]>
	</queryString>
