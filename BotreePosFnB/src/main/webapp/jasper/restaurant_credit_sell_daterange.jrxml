	<parameter name="W_StartDate" class="java.lang.String"/>
	<parameter name="W_EndDate" class="java.lang.String"/>
	<parameter name="W_StoreId" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT fo.id,fo.order_date,cm.name,cm.contact_no,cm.store_id,
bill.gross_amt,bill.bill_amt,
bfp.paid_amt,(bill.gross_amt - bfp.paid_amt) as due
FROM fo_t_orders fo
LEFT JOIN ci_m_store_customer cm
ON
fo.store_customer_id=cm.id
LEFT JOIN bp_t_paid_amount bfp
ON
fo.id=bfp.order_id
LEFT JOIN bp_t_bill bill
ON bill.Order_Id=fo.id
where  cm.credit_customer='Y'
and fo.order_date between $P{W_StartDate} and $P{W_EndDate}
and cm.store_id=$P{W_StoreId}
order by cm.name asc]]>
	</queryString>
